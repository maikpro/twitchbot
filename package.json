{
    "name": "twitchbot",
    "version": "1.0.0",
    "description": "A Bot for Twitch.tv",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "prod": "node src",
        "start": "node -r ts-node/register --env-file=.env src",
        "dev": "nodemon src -e ts,json --exec \"npm start\"",
        "lint": "eslint src",
        "format": "npx prettier --write src",
        "format:check": "npx prettier --check src"
    },
    "keywords": [],
    "author": "maikpro",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/node": "20.11.16",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "6.20.0",
        "@typescript-eslint/parser": "6.20.0",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "express": "4.18.2",
        "nodemon": "3.0.3",
        "prettier": "3.2.4",
        "ts-node": "10.9.2",
        "typescript": "5.3.3"
    },
    "dependencies": {
        "websocket": "^1.0.34",
        "ws": "^8.16.0"
    }
}
